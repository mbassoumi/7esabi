    "antd": "^4.16.13",
    "axios": "^0.24.0",
    "date-fns": "^2.26.0",
    "i18next": "^21.3.3",
    "i18next-http-backend": "^1.3.1",
    "lodash": "^4.17.21",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-highlight-words": "^0.17.0",
    "react-i18next": "^11.12.0",
